#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_node-gyp-build@3.7.0@node-gyp-build/optional.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_node-gyp-build@3.7.0@node-gyp-build/optional.js" "$@"
  ret=$?
fi
exit $ret
